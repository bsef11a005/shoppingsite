                <!-- End Content -->
                <!-- Sidebar -->
                <div id="sidebar">
                    <!-- Search -->
                    <div class="box search">
                        <h2>Search by <span></span></h2>
                        <div class="box-content">
                            <form action="/Product/Search" method="post">
                                <label>Keyword</label>
                                <input type="text" class="field" name="sea" />
                                <label>Category</label>
                                <select class="field" name="sel">
                                    <option value="">-- Select Category --</option>
                                    <option value="Electronics">Electronics</option>
                                    <option value="Garments">Garments</option>
                                    <option value="Shoes">Shoes</option>
                                    <option value="Furniture">Furniture</option>
                                    <option value="Glasses">Glasses</option>
                                    <option value="Books">Books</option>
                                    <option value="Watches">Watches</option>
                                    <option value="Jewelry">Jewelry</option>
                                    <option value="Toys">Toys</option>
                                    <option value="Pets">Pets</option>
                                    <option value="Sports">Sports</option>
                                    <option value="Beauty"></option>
                                    <option value="Vehicle"></option>
                                </select>
                                
                                <input type="submit" class="search-submit" value="Search" />
                                <p>
                                    <a href="#" class="bul">Advanced search</a><br />
                                    <a href="#" class="bul">Contact Customer Support</a>
                                </p>
                            </form>
                        </div>
                    </div>
                    <!-- End Search -->
                    <!-- Categories -->
                    <div class="box categories">
                        <h2>Categories <span></span></h2>
                        <div class="box-content">
                            <ul>
                                <li><a name="t" href="/Product/Categories?u=Electronics">Electronics</a></li>
                                <li><a name="t" href="/Product/Categories?u=Garments">Garments</a></li>
                                <li><a name="Shoes" href="/Product/Categories?u=Shoes">Shoes</a></li>
                                <li><a name="Furniture" href="/Product/Categories?u=Furniture">Furniture</a></li>
                                <li><a name="Glasses" href="/Product/Categories?u=Glasses">Glasses</a></li>
                                <li><a name="Books" href="/Product/Categories?u=Books">Books</a></li>
                                <li><a name="Watches" href="/Product/Categories?u=Watches">Watches</a></li>
                                <li><a name="Jewelry" href="/Product/Categories?u=Jewelry">Jewelry</a></li>
                                <li><a name="Toys" href="/Product/Categories?u=Toys">Toys</a></li>
                                <li><a name="Pets" href="/Product/Categories?u=Pets">Pets</a></li>
                                <li><a name="Sports" href="/Product/Categories?u=Sports">Sports</a></li>
                                <li><a name="Beauty" href="/Product/Categories?u=Beauty">Beauty</a></li>
                                <li class="last"><a name="Vehicle" href="/Product/Categories?u=Vehicle">Vehicle</a></li>
                            </ul>
                        </div>
                    </div>
