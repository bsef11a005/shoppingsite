<body>
    <!-- Shell -->
    <div class="shell">
        <!-- Header -->
        <div id="header">
            <h1 id="logo"><a href="#">shoparound</a></h1>
            <!-- Cart -->
            <div id="cart">
                @RenderSection("s1", false)
                <div class="cl">&nbsp;</div>
                <span>Articles: <strong>4</strong></span> &nbsp;&nbsp; <span>Cost: <strong>$250.99</strong></span>
                
            </div>
            <!-- End Cart -->
            <!-- Navigation -->
            <div id="navigation">
                <ul>
                    <li><a href="/Home/Index" class="active">Home</a></li>
